            "scratchpad": {
                "mainBundleRequired": false,
                "port": 8088
            },
            "page": {
                "enabled": true,
                "mainBundleRequired": false,
                "port": 8088
            },
            "test": {
                "directory": "./test",
                "bundleRequired": false
            },
            "transform": {
                "enableNode": false,
                "nodeDirectory": "./node"
            }
        },
        "source": "./source"
    }
