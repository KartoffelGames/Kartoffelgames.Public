{
    "$schema": "https://turborepo.org/schema.json",
    "baseBranch": "origin/master",
    "pipeline": {
        "build:debug": {
            "dependsOn": [
                "^build:debug"
            ]
        },
        "build": {
            "dependsOn": [
                "^build"
            ]
        },
        "test": {
            "dependsOn": [
                "^build:debug"
            ]
        }
    }
}