{
    "name": "kartoffelgames",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "preinstall": "git config core.hooksPath ./enviroment/git_hooks && git config pull.rebase true",
        "lint": "npx eslint \"**/*.ts\" --config enviroment/configuration/eslint.config.js",
        "lint:fix": "npm run lint -- --fix",
        "workflow:update-version-numbers": "node ./enviroment/scripts/project/project.mjs run=2",
        "project": "node ./enviroment/scripts/project/project.mjs"
    },
    "private": true,
    "workspaces": [
        "./packages/*"
    ],
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "~5.21.0",
        "@typescript-eslint/parser": "~5.21.0",
        "eslint": "^8.23.0",
        "turbo": "^1.0.28"
    },
    "packageManager": "npm@8.1.2"
}